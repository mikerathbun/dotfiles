#!/bin/sh
tmux new-session -s 'Software Development' -d  # Create a new session named but don't attach (-d)
tmux split-window -h
tmux select-pane -t 0
tmux split-window -v
tmux select-pane -t 2
tmux resize-pane -L 40
tmux -2 attach-session -d # Attach to the created session  using 256 colors (-2) and detach (-d) any other sessions.
